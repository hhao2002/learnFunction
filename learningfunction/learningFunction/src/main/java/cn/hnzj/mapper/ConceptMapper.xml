<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.edu.hnzj.mapper.IdentityCardMapper">

	<select id="findIdentityCardById" resultType="identityCard">
		select id,number
		from identityCard
		where id = #{id}
	</select>

</mapper>
